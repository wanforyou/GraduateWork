<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.tyut.himusic.activity.CircleMenuActivity">


    <RelativeLayout
        android:layout_width="240dp"
        android:layout_height="240dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true">


        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView4"
            android:src="@drawable/ic_album_grey600_36dp"
            android:layout_alignTop="@+id/imageView5"
            android:layout_alignStart="@+id/imageView5" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView5"

            android:src="@drawable/ic_archive_grey600_24dp"
            android:layout_alignTop="@+id/imageView6"
            android:layout_alignStart="@+id/imageView6" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView6"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:src="@drawable/circle_yellow_48px_543519_easyicon"
            android:layout_alignTop="@+id/imageView3"
            android:layout_alignStart="@+id/imageView3" />
        <ImageView
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:id="@+id/imageView3"
            android:src="@drawable/fab_anim"
            android:background="@drawable/circle_yellow_48px_543519_easyicon"
            android:layout_marginEnd="59dp"
            android:layout_marginBottom="56dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true" />

    </RelativeLayout>

</RelativeLayout>
